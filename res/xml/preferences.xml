<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >
    <PreferenceCategory android:title="@string/settings19" >
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="data_server_enabled"
            android:summary="@string/settings23"
            android:title="@string/settings22" />

        <EditTextPreference
            android:defaultValue="http://"
            android:inputType="textUri"
            android:key="data_server"
            android:summary="@string/dataservertitle"
            android:title="@string/dataservertitle" />
        <EditTextPreference
            android:defaultValue="123456"
            android:inputType="textVisiblePassword"
            android:key="passcode"
            android:summary="@string/datapasssumary"
            android:title="@string/datapasstitle" />
    </PreferenceCategory>
    
    <PreferenceCategory android:title="@string/settings3" >
        <CheckBoxPreference
            android:defaultValue="false"
            android:key="stream_audio"
            android:summary="@string/settings4"
            android:title="@string/settings3" />

        <ListPreference
            android:entries="@array/audioEncoderArray"
            android:entryValues="@array/audioEncoderValues"
            android:key="audio_encoder"
            android:summary="@string/settings6"
            android:title="@string/settings5" />
    </PreferenceCategory>
    
   
    <PreferenceCategory android:title="@string/settings7">
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="stream_video"
            android:summary="@string/settings8"
            android:title="@string/settings7" />

        <ListPreference
            android:entries="@array/videoEncoderArray"
            android:entryValues="@array/videoEncoderValues"
            android:key="video_encoder"
            android:summary="@string/settings10"
            android:title="@string/settings9" />
        <ListPreference
            android:entries="@array/videoResolutionArray"
            android:entryValues="@array/videoResolutionArray"
            android:key="video_resolution"
            android:title="@string/settings11" />
        <ListPreference
            android:entries="@array/videoFramerateArray"
            android:entryValues="@array/videoFramerateValues"
            android:key="video_framerate"
            android:title="@string/settings12" />
        <ListPreference
            android:entries="@array/videoBitrateArray"
            android:entryValues="@array/videoBitrateValues"
            android:key="video_bitrate"
            android:title="@string/settings13" />
    </PreferenceCategory>
    
    
    
    <PreferenceCategory android:title="@string/settings28" >              
        <EditTextPreference
            android:defaultValue="8086"
            android:inputType="number"
            android:key="rtsp_port"
            android:summary="@string/settings32"
            android:title="@string/settings31" />      
    </PreferenceCategory>    
    
    <PreferenceCategory android:title="@string/settings14" >
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="notification_enabled"
            android:summary="@string/settings16"
            android:title="@string/settings15" />
    </PreferenceCategory>

</PreferenceScreen>